{
  "name": "esm-graph-inspector",
  "type": "module",
  "version": "0.0.1",
  "bin": "./bin.mjs",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "dev:app": "nuxi dev",
    "dev:cli": "npm run build:app && unbuild --watch",
    "build:app": "nuxi build",
    "build:cli": "unbuild",
    "build": "npm run build:cli && npm run build:app",
    "start": "node bin.mjs",
    "watch": "node --watch bin.mjs",
    "prepare": "nuxi prepare",
    "lint": "nuxi prepare && eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@thepassle/module-graph": "^0.11.0",
    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "cleye": "^1.3.4",
    "crossws": "^0.4.1",
    "get-port-please": "^3.2.0",
    "h3": "^2.0.0-beta.1",
    "mrmime": "^2.0.1",
    "open": "^10.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@iconify-json/tabler": "^1.2.19",
    "@nuxt/eslint": "^1.5.2",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/ui": "^3.2.0",
    "@types/node": "^24.0.13",
    "eslint-plugin-format": "^1.0.1",
    "nuxt": "^3.17.6",
    "tailwind-scrollbar": "^4.0.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vue-tsc": "^2.2.12"
  }
